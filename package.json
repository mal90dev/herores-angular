    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "eslint": "8.56.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "json-server": "0.17.4",
    "typescript": "5.2.2"
  }
